{
  "name": "load-balancer",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev:load-balancer": "bun --hot src/index.ts",
    "dev:4001": "bun --hot src/4001.ts",
    "dev:4002": "bun --hot src/4002.ts",
    "dev:4003": "bun --hot src/4003.ts",
    "dev": "concurrently \"bun run dev:load-balancer\" \"bun run dev:4001\" \"bun run dev:4002\" \"bun run dev:4003\"",
    "build": "rimraf ./dist && bun build src/index.ts --outdir ./dist"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^4.17.21",
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "-": "^0.0.1",
    "@types/express-http-proxy": "^1.6.6",
    "@types/http-proxy": "^1.17.14",
    "axios": "^1.7.2",
    "express": "^4.19.2",
    "express-http-proxy": "^2.0.0",
    "rimraf": "^5.0.7",
    "winston": "^3.13.0"
  }
}
